    &lt;meta charset=&quot;utf-8&quot;&gt;
    &lt;title&gt;Crear y descargar archivos con Javascript&lt;/title&gt;
    &lt;script type=&quot;text/javascript&quot;&gt;
      function descargarArchivo(contenidoEnBlob, nombreArchivo) {
        //CÓDIGO AQUÍ
      };
