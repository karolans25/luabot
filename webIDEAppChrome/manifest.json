{
		"name": "LuaBot",
		"version": "0.1",
		"manifest_version": 2,
		"description": "IDE para esp8266 y nodeMCU",
		"icons":{
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
            },
		"app": {
			"background": {
				"scripts": ["app/background.js"]
			}
		},
		"permissions":	[
				"serial",
				"clipboardRead",
				"clipboardWrite",
				{"fileSystem": ["write"]}
		]
}
